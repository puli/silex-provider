{
    "name": "puli/silex-provider",
    "description": "The Puli service provider for Silex.",
    "homepage": "http://puli.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Bernhard Schussek",
            "email": "bschussek@gmail.com"
        }
    ],
    "require": {
        "php": "^5.3.9|^7.0",
        "puli/discovery": "^1.0.0-beta9@dev",
        "puli/repository": "^1.0.0-beta9@dev,<1.1",
        "puli/url-generator": "^1.0.0-beta4@dev",
        "silex/silex": "^1.3",
        "webmozart/expression": "^1.0.0-beta5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.6",
        "puli/cli": "^1.0.0-beta10@dev",
        "puli/composer-plugin": "^1.0.0-beta9@dev",
        "puli/manager": "^1.0-beta10@dev",
        "puli/twig-extension": "^1.0.0-beta7",
        "sebastian/version": "^1.0.1",
        "webmozart/console": "^1.0-beta2"
    },
    "autoload": {
        "psr-4": {
            "Puli\\SilexProvider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Puli\\SilexProvider\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "support": {
        "issues": "https://github.com/puli/issues/issues"
    }
}
